Proceso Intercambiar
	Definir num1 Como Entero;
	Definir num2 Como Entero;
	Definir temporal Como Entero;
	
	Escribir ("Proporciona el valor del primer numero: ");
	Leer num1;
	Escribir ("Proporciona el valor del segundo numero: ");
	leer num2;
	
	temporal <- num1;
	num1 <- num2;
	num2 <- temporal;
	
	
	
	escribir("Los valores intercambiados son: "), num1,", ", num2;
	
	
FinProceso
